{
    "name": "talevskiigor/composer-bump",
    "description": "Laravel artisan command to increment composer.json version.",
    "keywords": ["bump", "artisan", "laravel","composer"],
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "authors": [
        {
            "name": "Igor T.",
            "email": "igor.talevski@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Talevskiigor\\ComposerBump\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
